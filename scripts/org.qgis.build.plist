<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
        <key>Label</key>
        <string>org.qgis.build</string>

        <key>ProgramArguments</key>
        <array>
                <string>/Users/admin/qgis/run_cronjob.bash</string>
        </array>

        <key>KeepAlive</key>
        <false/>

        <key>RunAtLoad</key>
        <false/>

        <key>StartCalendarInterval</key>
        <dict>
            <key>Minute</key>
            <integer>10</integer>
            <key>Hour</key>
            <integer>12</integer>
        </dict>

        <key>StandardErrorPath</key>
        <string>/Users/admin/qgis/logs/qgis_launchd.err</string>

        <key>StandardOutPath</key>
        <string>/Users/admin/qgis/logs/qgis_launchd.out</string>
</dict>
</plist>
